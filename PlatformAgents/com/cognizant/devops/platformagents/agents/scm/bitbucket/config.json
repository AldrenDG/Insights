{
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight"
  },
  "subscribe": {
    "config": "SCM.BITBUCKET.config"
  },
  "publish": {
    "data": "SCM.BITBUCKET.DATA",
    "health": "SCM.BITBUCKET.HEALTH"
  },
  "communication": {
    "type": "REST"
  },
  "responseTemplate": {
    "id": "commitId",
    "author": {
      "name": "authorName",
      "emailAddress": "authorEmail"
    },
    "authorTimestamp": "commitTime",
    "message": "commitMessage",
    "properties": {
      "jira-key": [
        "jiraKey"
      ]
    },
    "pullRequests": {
      "id" : "pullReqId",
      "state" : "pullReqState",
      "createdDate" : "createdDate",
      "updatedDate" : "updatedDate",
      "closedDate" : "closedDate",
      "fromRef" : {
        "displayId" : "fromBranch"
      },
      "toRef" : {
        "displayId" : "toBranch"
      },
      "author": {
        "user": {
          "name": "authorName",
          "emailAddress": "authorEmail"
        }
      }
    }
  },
  "runSchedule": 30,
  "toolsTimeZone" : "GMT",
  "insightsTimeZone" : "GMT",
  "StartFrom" : "2017-07-01 00:00:00",
  "useResponseTemplate" : true,
  "scanAllBranches" : false,
  "scanPullRequests": true,
  "auth": "base64",
  "UserID": "",
  "Passwd": "",
  "timeStampField":"commitTime",
  "isEpochTimeFormat" : true,
  "BaseEndPoint": "http://127.0.0.1:7990/rest/api/1.0/projects/",
  "isDebugAllowed" : false,
  "loggingSetting" : {
        "logLevel" : "WARN"
    }
}
